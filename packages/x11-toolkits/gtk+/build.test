CONFIGURE=" ./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=/usr
            --bindir=/usr/bin
            --sbindir=/usr/sbin
            --sysconfdir=/etc
            --localstatedir=/var
            --libdir=/usr/lib
            --includedir=/usr/include
            --datarootdir=/usr/share
            --datadir=/usr/share
            --infodir=/usr/share/info
            --localedir=/usr/share/locale
            --mandir=/usr/share/man
            --disable-static \
            --enable-shared"



